{
    "compilerOptions": {
        "target": "ES2020",
        "strict": true,
        "outDir": "./build/",
        "module": "commonjs",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "noImplicitAny": false
    }
}